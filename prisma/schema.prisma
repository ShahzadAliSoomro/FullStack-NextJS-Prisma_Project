datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model product {
    id String @id @default(auto()) @map("_id") @db.ObjectId
    image String?
    title String
    price Float
    category String
    isAvailable Boolean
    description String?
    createdAt DateTime @default(now())
}
